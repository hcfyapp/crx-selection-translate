{
  "name" : "划词翻译" ,
  "description" : "支持谷歌、百度、有道、必应四大翻译和朗读引擎，可以方便的查看、复制和朗读不同引擎的翻译结果。" ,
  "manifest_version" : 2 ,
  "version" : "6.0.11" ,
  "homepage_url" : "https://github.com/lmk123/crx-selection-translate" ,
  "content_security_policy" : "script-src 'self' 'unsafe-eval' https://www.google-analytics.com; object-src 'self'" ,
  "icons" : {
    "128" : "/logo.png" ,
    "16" : "/logo.png" ,
    "48" : "/logo.png"
  } ,
  "author" : "Milk Lee <milk.lee@qq.com>" ,
  "permissions" : [
    "*://*/*" ,
    "contextMenus" ,
    "storage" ,
    "clipboardWrite" ,
    "clipboardRead" ,
    "activeTab" ,
    "identity",
    /* for pdf.js */
    "fileBrowserHandler",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
    "tabs",
    "webNavigation",
    "streamsPrivate"
  ] ,
  "background" : {
    "page": "/allHandler.html"
  } ,
    "content_scripts" : [
      /* for pdf */ {
        "matches": [
          "http://*/*.pdf",
          "https://*/*.pdf",
          "ftp://*/*.pdf",
          "file://*/*.pdf"
        ],
        "run_at": "document_start",
        "all_frames": true,
        "css": ["/chromium/contentstyle.css"],
        "js": ["/chromium/contentscript.js"]
    },{
        "matches" : [
        "http://*/*",
        "https://*/*",
        "file://*"
        ] ,
        "css" : [
        "/bundle/commons1.js.css"
        ] ,
        "js" : [
        "/bundle/commons3.js" ,
        "/bundle/commons2.js" ,
        "/bundle/commons1.js" ,
        "/bundle/content.js"
        ] ,
        "all_frames" : true ,
        "run_at" : "document_start"
    }] ,
    /* for pdf */
  "file_browser_handlers": [{
    "id": "open-as-pdf",
    "default_title": "Open with PDF Viewer",
    "file_filters": [
      "filesystem:*.pdf"
    ]
  }],
  "storage": {
    "managed_schema": "chromium/preferences_schema.json"
  },
  "incognito": "split",
  "web_accessible_resources": [
    "/bundle/*.woff" ,
    "/content-scripts/web/embed/*",
    "getFrameId",
    "/chromium/content/web/viewer.html",
    "http:/*",
    "https:/*",
    "ftp:/*",
    "file:/*",
    "chrome-extension:/*",
    "filesystem:/*",
    "drive:*"
  ],

  "options_page" : "/options/index.html" ,
  "options_ui" : {
    "page" : "/options/index.html" ,
    "chrome_style" : true
  } ,
  "browser_action" : {
    "default_icon" : {
      "19" : "/logo.png" ,
      "38" : "/logo.png"
    } ,
    "default_popup" : "/popup/index.html"
  } ,
  "commands" : {
    "_execute_browser_action" : {
      "suggested_key" : {
        "default" : "Ctrl+Q" ,
        "mac" : "MacCtrl+Q"
      }
    } ,
    "translate" : {
      "suggested_key" : {
        "default" : "Alt+A"
      } ,
      "description" : "翻译网页中选中的文本"
    } ,
    "web" : {
      "suggested_key" : {
        "default" : "Alt+Z"
      } ,
      "description" : "翻译整个网页"
    }
  } ,
  "minimum_chrome_version" : "29"
}
